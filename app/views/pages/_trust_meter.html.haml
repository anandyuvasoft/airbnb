- @leads = current_doctor.booked_patients


.trust-meter-box
  %select#year.form-control{:name => "year"}
    %option{:value => "1"} Roberto Casaras MD
  %span
    %center
      Has a
      %b 20%
      Trust Meter Score
      -#%sup I
  .progress
    .progress-bar.progress-bar-primary{"aria-valuemax" => "100", "aria-valuemin" => "0", "aria-valuenow" => "60", :role => "progressbar", :style => "width: 20%"}
  %span.blue-text
    %center Learn How to Raise Your TrustMeter Score
.leads-box
  %span Total Leads
  %select#year.form-control{:name => "year"}
    %option{:value => "1"} All Times
  %h1= @leads.size
  .leads-box-bottom
    %a{:href => leads_path(current_doctor)} See All My Leads



= render "leads"


.my-listing-box
  %h1 187
  %span People Have Visited on my profile
  %select#year.form-control{:name => "year"}
    %option{:value => "1"} All Times
  .my-listing-bottom
    %a{:href => "javascript:void(0)"}
      Unlock Marketing Insights
      %i.fa.fa-lock 