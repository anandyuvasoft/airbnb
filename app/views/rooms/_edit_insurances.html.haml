= form_for(@room, url: room_path, method: :put) do |room|                  
  .form-group 
    = room.select :insurance_ids, Insurance.all.map{|insurance_provider| [insurance_provider.name,insurance_provider.id] }, {}, {:multiple => true, class: 'chosen-select'}

  .pull-right
    = room.submit "Save" , class: "btn btn-primary btn-sm"