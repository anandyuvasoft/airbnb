= form_for(@room, url: room_path, method: :put) do |room|                  
  .form-group 
    = room.select :language_ids, Language.all.map{|language| [language.name, language.id] }, {}, {:multiple => true, class: 'chosen-select'}
  .pull-right
    = room.submit "Save" , class: "btn btn-primary btn-sm"